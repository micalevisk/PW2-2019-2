    "debug": "^2.6.9",
    "express": "^4.16.1",
    "express-handlebars": "^3.1.0",
    "http-errors": "^1.6.3",
    "jquery": "^3.4.1",
    "morgan": "^1.9.1",
    "mysql2": "^1.7.0",
    "node-sass-middleware": "^0.11.0",
    "popper.js": "^1.15.0",
    "sequelize": "^5.19.1"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.11.2",
