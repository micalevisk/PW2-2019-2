{
  "name": "chess-game",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "main": "./bin/www",
  "scripts": {
    "start": "DEBUG=chess:* node .",
    "dev": "DEBUG=chess:* NODE_ENV=development nodemon .",
    "sass:w": "node-sass --watch public/scss/ --output public/css/",
